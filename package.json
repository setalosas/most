{
    "name": "most",
    "displayName": "Most Theme",
    "description": "A semantic coloring code theme.",
    "version": "0.1.1",
    "publisher": "setalosas",
    "preview": true,
    "license": "SEE LICENSE IN<LICENSE.md>",
    "icon": "Most.png",
    "homepage": "https://github.com/setalosas/most/blob/master/README.md",
    "bugs": {
        "url": "https://github.com/setalosas/most/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/setalosas/most.git"
    },
    "engines": {
        "vscode": "^1.12.0"
    },
	"extensionKind": [
		"ui",
		"workspace"
	],
    "categories": ["Themes", "Other"],
    "galleryBanner": {
        "color": "#EBEEF5",
        "theme": "light"
    },
    "contributes": {
        "themes": [{
            "label": "Most Dark",
            "uiTheme": "vs-dark",
            "path": "./themes/Most-Dark.json"
        }, {
            "label": "Most Dark Bold",
            "uiTheme": "vs-dark",
            "path": "./themes/Most-Dark-Bold.json"
        },{
            "label": "Most Dark Italic",
            "uiTheme": "vs-dark",
            "path": "./themes/Most-Dark-Italic.json"
        }, {
            "label": "Most Light",
            "uiTheme": "vs",
            "path": "./themes/Most-Light.json"
        }, {
            "label": "Most Light Bold",
            "uiTheme": "vs",
            "path": "./themes/Most-Light-Bold.json"
        }, {
            "label": "Most Light Italic",
            "uiTheme": "vs",
            "path": "./themes/Most-Light-Italic.json"
        }]
    },
    "keywords": ["Most", "dark", "light", "theme", "vscode"],
    "scripts": {
        "changelog": "conventional-changelog -p angular -i CHANGELOG.md -w"
    },
    "__metadata": {
        "publisherDisplayName": "setalosas"
    }
}